  <% if @game.turn[:win] %>
    <!-- <div id="win-overlay"> -->

      GAME OVER
      <div class="board">
        <% @game.moves.each do |square| %>
        <a class="btn square"><%= square %></a>
        <% end %>
      </div>
      <!-- <%= @game.turn[:choice] %> -->
      <!-- WINS -->
    <!-- </div> -->
    <% else %>
    <div class="board">
      <% @game.moves.each.with_index do |square, index| %>
      <a id=<%= "square-#{index}" %> class="square" href="/game/<%= index %>"><%= square %></a>
      <% end %>
    </div>
  <% end %>
